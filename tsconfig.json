{
  "compilerOptions": {
    "outDir": "lib",
    "baseUrl": "./src",
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/sd.js", "src/views/Doce/index.tsx", "src/pages/widget/[userName].ㅗ싀", "src/pages/widget/[userName].tsx", "src/views/Remotecontrol/index.tsx", "src/pages/LoginPage.tsx", "src/widgets/widfetspage.tsx", "src/widgets/[userName].tsx"],
  "exclude": ["node_modules", ".next",],
  "ts-node": {
    "require": ["tsconfig-paths/register"],
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
